    "react-alice-carousel": "^2.8.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "tailwindcss": "^3.4.3",
    "vite": "^5.0.0"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.35.6",
